  "main": "index.js",
  "ignore": [
    "**/.*",
    "node_modules",
    "components"
  ],
  "dependencies": {
    "html5shiv": "*",
    "jquery": "*",
    "foundation": null,
    "fontawesome": "*"
  },
  "exportsOverride": {
    "jquery": {
      "": "**/jquery.min.js"
    },
